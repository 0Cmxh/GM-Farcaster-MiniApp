# Neynar API Configuration
# Get your API key at https://neynar.com
NEXT_PUBLIC_NEYNAR_API_KEY=your_neynar_api_key_here
NEYNAR_API_KEY=your_neynar_api_key_here

# App Configuration
NEXT_PUBLIC_APP_URL=https://your-app-url.vercel.app

# Farcaster Configuration (optional - for production manifest)
# These are generated using the Farcaster Manifest Tool
FARCASTER_MANIFEST_HEADER=your_signed_header_here
FARCASTER_MANIFEST_PAYLOAD=your_signed_payload_here
FARCASTER_MANIFEST_SIGNATURE=your_signature_here

# Optional: Custom RPC endpoints
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org
